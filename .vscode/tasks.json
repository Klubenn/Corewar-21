{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${workspaceFolder}/src/*.c",
				"${workspaceFolder}/libft/libft.a",
				"${workspaceFolder}/ft_printf_1/libftprintf.a",
				"-o",
				"${workspaceFolder}/test"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
	]
}