				"${workspaceFolder}/memory_deleter.c",
				"${workspaceFolder}/main.c",
				"${workspaceFolder}/alives_checker.c",
				"${workspaceFolder}/vm_field_memory_allocator.c",
				"${workspaceFolder}/winner_definer.c",
				"${workspaceFolder}/processes_creator.c",
				"${workspaceFolder}/processes_deleter.c",
				"${workspaceFolder}/validation_for_args_type.c",
				"${workspaceFolder}/move_programming_counter.c",
				"${workspaceFolder}/ft_printf/libftprintf.a",
				"-o",
				"${workspaceFolder}/test"
			],
			"options": {
